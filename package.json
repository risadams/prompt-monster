  },
  "engines": {
    "node": ">=16.0.0"
  },
  "author": {
    "name": "Ris Adams",
    "email": "ris@risadams.com"
  },
  "dependencies": {
    "@awesome.me/kit-1792c0f8ba": "^1.0.7",
    "@fortawesome/fontawesome-svg-core": "^7.0.0",
    "@fortawesome/react-fontawesome": "^0.2.3",
    "@tailwindcss/cli": "^4.1.11",
    "core-js-pure": "^3.44.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "tailwindcss": "^4.1.11",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.27.1",
    "gh-pages": "^6.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1"
  },
  "scripts": {
    "tailwind:build": "npx @tailwindcss/cli -i ./src/styles/src.css -o ./public/app.css",
    "tailwind:watch": "npx @tailwindcss/cli -i ./src/styles/src.css -o ./public/app.css --watch",
    "start": "start npm run tailwind:watch && react-scripts start",
    "build": "npm run tailwind:build && react-scripts build",
    "test": "npx jest",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
